{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "VERCEL": "1"
  },
  "functions": {
    "api/**/*.py": {
      "maxDuration": 30
    },
    "*.py": {
      "maxDuration": 30,
      "includeFiles": "*.csv"
    }
  }
}
